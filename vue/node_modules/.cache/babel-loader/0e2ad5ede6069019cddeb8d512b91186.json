{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"ProfilView\",\n  data: function () {\n    return {\n      uloga: \"neovlascen_pristup\",\n      user: {\n        korisnickoIme: \"\",\n        ime: \"\",\n        prezime: \"\",\n        p: \"\",\n        datum_rodjenja: \"\",\n        uloga: \"\",\n        lozinka: \"\"\n      }\n    };\n  },\n  mounted: function () {\n    axios.get(\"http://localhost:8081/api/profil\", {\n      withCredentials: 'include'\n    }).then(res => {\n      console.log(res.data);\n      this.user = res.data;\n      this.user.datum_rodjenja = this.user.datum_rodjenja.substring(0, 10);\n    }).catch(err => {\n      console.log(err);\n      alert(\"Neispravna izmena\");\n    });\n  },\n  methods: {\n    cancel: function () {\n      this.user = this.resUser;\n      window.location.reload();\n    },\n    save: function () {\n      axios.post(\"http://localhost:8081/api/izmeni-profil\", this.user, {\n        withCredentials: 'include'\n      }).then(res => {\n        console.log(res);\n        alert(\"Uspesno ste izmenili podatke!\");\n      }).catch(error => {\n        console.log(error);\n        alert(\"Neispravan unos!\");\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AAgGA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,IAAI,EAAE,YADO;EAEbC,IAAI,EAAE,YAAY;IAChB,OAAO;MACLC,KAAK,EAAE,oBADF;MAELC,IAAI,EAAG;QACLC,aAAa,EAAE,EADV;QAELC,GAAG,EAAE,EAFA;QAGLC,OAAO,EAAE,EAHJ;QAILC,CAAC,EAAE,EAJE;QAKLC,cAAc,EAAE,EALX;QAMLN,KAAK,EAAE,EANF;QAOLO,OAAO,EAAE;MAPJ;IAFF,CAAP;EAYD,CAfY;EAgBZC,OAAO,EAAE,YAAY;IACpBX,KAAI,CACDY,GADH,CACO,kCADP,EAC2C;MAACC,eAAe,EAAC;IAAjB,CAD3C,EAEGC,IAFH,CAESC,GAAD,IAAS;MACbC,OAAO,CAACC,GAAR,CAAYF,GAAG,CAACb,IAAhB;MACA,KAAKE,IAAL,GAAYW,GAAG,CAACb,IAAhB;MACA,KAAKE,IAAL,CAAUK,cAAV,GAA2B,KAAKL,IAAL,CAAUK,cAAV,CAAyBS,SAAzB,CAAmC,CAAnC,EAAqC,EAArC,CAA3B;IACD,CANH,EAOGC,KAPH,CAOUC,GAAD,IAAQ;MACbJ,OAAO,CAACC,GAAR,CAAYG,GAAZ;MACAC,KAAK,CAAC,mBAAD,CAAL;IACD,CAVH;EAWA,CA5BW;EA6BZC,OAAO,EAAE;IACJC,MAAM,EAAE,