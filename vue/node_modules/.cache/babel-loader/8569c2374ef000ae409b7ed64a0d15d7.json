>\r\n\r\n\r\n\r\n<script>\r\n\r\nimport axios from 'axios';\r\nexport default {\r\n    name: \"KreirajRestoran\",\r\n    data: function() {\r\n        return {\r\n            uloga: \"neovlascen_pristup\",\r\n            restoran: {\r\n                naziv: \"\",\r\n                tipRestorana: \"\",\r\n                lokacija: {\r\n                    adresa: \"\",\r\n                    sirina: 0,\r\n                    duzina: 0\r\n                }\r\n            }\r\n        }\r\n    },\r\n    mounted: function() {\r\n        axios // uloga\r\n            .get('http://localhost:8081/api/vratiUlogu/', {withCredentials: true})\r\n            .then((res) => {\r\n                this.uloga = res.data\r\n           })\r\n            .catch((err) =>{ // todo neovlascen pristup\r\n                console.log(err)\r\n            })\r\n    },\r\n    methods: {  \r\n        handleSubmit: function() {\r\n            console.log(this.restoran)\r\n            axios\r\n            .post(\"http://localhost:8081/api/dodaj-restoran\", this.restoran, {withCredentials: true})\r\n            .then(res => {\r\n                console.log(res)\r\n                this.$router.push(\"/\")\r\n            })\r\n            .catch(error => {\r\n                console.log(error)\r\n                alert(\"Neispravni podaci\")\r\n            }); \r\n        }\r\n    }\r\n};    \r\n</script>\r\n"]},"metadata":{},"sourceType":"module"}