{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: 'KupacView',\n  data: function () {\n    return {\n      porudzbine: {\n        artikli: [],\n        restoran: [],\n        datum_vreme: \"\",\n        cena: 0,\n        status: \"\"\n      }\n    };\n  },\n  mounted: function () {\n    // ovde kao callback funkciju odmah poziva i pravi employee\n    axios.get('http://localhost:8081/api/porudzbine/', {\n      withCredentials: true\n    }).then(res => {\n      console.log(res.data);\n      this.menadzer = res.data;\n      console.log(this.menadzer.restoran.naziv);\n    }).catch(err => {\n      // todo neovlascen pristup\n      console.log(err);\n    });\n  },\n  methods: {\n    deleteEmployee: function (naziv) {\n      //this.restoran_naziv.naziv = naziv;\n      //console.log(this.restoran_naziv)\n      axios.get('http://localhost:8081/api/restoran/obrisi-artikal/' + naziv, {\n        withCredentials: true\n      }).then(res => {\n        console.log(res);\n        window.location.reload();\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AA+DA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;EACXC,IAAI,EAAE,WADK;EAEXC,IAAI,EAAE,YAAW;IACb,OAAO;MACHC,UAAU,EAAE;QACAC,OAAO,EAAE,EADT;QAEAC,QAAQ,EAAE,EAFV;QAGAC,WAAW,EAAE,EAHb;QAIAC,IAAI,EAAE,CAJN;QAKAC,MAAM,EAAE;MALR;IADT,CAAP;EASH,CAZU;EAaXC,OAAO,EAAE,YAAW;IAChB;IAEAT,KAAI,CACCU,GADL,CACS,uCADT,EACkD;MAACC,eAAe,EAAE;IAAlB,CADlD,EAEKC,IAFL,CAEWC,GAAD,IAAS;MACXC,OAAO,CAACC,GAAR,CAAYF,GAAG,CAACX,IAAhB;MACA,KAAKc,QAAL,GAAgBH,GAAG,CAACX,IAApB;MACAY,OAAO,CAACC,GAAR,CAAY,KAAKC,QAAL,CAAcX,QAAd,CAAuBY,KAAnC;IACJ,CANJ,EAOKC,KAPL,CAOYC,GAAD,IAAQ;MAAE;MACbL,OAAO,CAACC,GAAR,CAAYI,GAAZ;IACH,CATL;EAWH,CA3BU;EA4BXC,OAAO,EAAE;IACLC,cAAc,EAAE,UAAUJ,KAAV,EAAiB;MAC/B;MACA;MACFjB,KAAI,CACCU,GADL,CACS,uDAAuDO,KADhE,EACuE;QAACN,eAAe,EAAE;MAAlB,CADvE,EAEKC,IAFL,CAEUC,GAAE,IAAK;QACTC,OAAO,CAACC,GAAR,CAAYF,GAAZ;QACAS,MAAM,CAACC,QAAP,CAAgBC,MAAhB;MACH,CALL,EAMKN,KANL,CAMWO