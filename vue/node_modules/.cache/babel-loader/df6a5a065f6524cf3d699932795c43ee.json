{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"ProfilView\",\n  data: function () {\n    return {\n      user: {\n        korisnickoIme: '',\n        lozinka: '',\n        ime: '',\n        prezime: '',\n        p: '',\n        datum_rodjenja: ''\n      },\n      resUser: '',\n      lozinka: \"12345678\",\n      value: '',\n      tmp: \"2000-04-04\"\n    };\n  },\n  mounted: function () {\n    axios.get(\"http://localhost:8081/api/profil\", {\n      withCredentials: 'include'\n    }).then(res => {\n      console.log(res);\n      this.user = res.data;\n      this.resUser = JSON.parse(JSON.stringify(res.data));\n    }).catch(err => {\n      console.log(err);\n    });\n  },\n  methods: {\n    cancel: function () {\n      this.user = this.resUser;\n      window.location.reload();\n    },\n    save: function () {\n      //this.user.datum_rodjenja = this.user.datum_rodjenja+\"T22:00:00\";\n      axios.put(\"http://localhost:8081/api/izmeni-profil\", this.user).then(res => {\n        console.log(res);\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AAqGA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,IAAI,EAAE,YADO;EAEbC,IAAI,EAAE,YAAY;IAChB,OAAO;MACLC,IAAI,EAAE;QACJC,aAAa,EAAE,EADX;QAEJC,OAAO,EAAE,EAFL;QAGJC,GAAG,EAAE,EAHD;QAIJC,OAAO,EAAE,EAJL;QAKJC,CAAC,EAAE,EALC;QAMJC,cAAc,EAAE;MANZ,CADD;MASLC,OAAO,EAAE,EATJ;MAULL,OAAO,EAAE,UAVJ;MAWLM,KAAK,EAAE,EAXF;MAYLC,GAAG,EAAE;IAZA,CAAP;EAcD,CAjBY;EAkBZC,OAAO,EAAE,YAAY;IACpBb,KAAI,CACDc,GADH,CACO,kCADP,EAC2C;MAACC,eAAe,EAAC;IAAjB,CAD3C,EAEGC,IAFH,CAESC,GAAD,IAAS;MACbC,OAAO,CAACC,GAAR,CAAYF,GAAZ;MACA,KAAKd,IAAL,GAAYc,GAAG,CAACf,IAAhB;MACA,KAAKQ,OAAL,GAAeU,IAAI,CAACC,KAAL,CAAWD,IAAI,CAACE,SAAL,CAAeL,GAAG,CAACf,IAAnB,CAAX,CAAf;IACD,CANH,EAOGqB,KAPH,CAOUC,GAAD,IAAQ;MACbN,OAAO,CAACC,GAAR,CAAYK,GAAZ;IACD,CATH;EAUA,CA7BW;EA8BZC,OAAO,EAAE;IACJC,MAAM,EAAE,YAAW;MACf,KAAKvB,IAAL,GAAY,KAAKO,OAAjB;MACAiB,MAAM,CAACC,QAAP,CAAgBC,MAAhB;IACH,CAJG;IAKJC,IAAI,EA