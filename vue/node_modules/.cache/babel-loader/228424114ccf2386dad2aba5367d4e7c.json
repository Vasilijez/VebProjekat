{"ast":null,"code":"//<td><button @click=\"dlete\" type=\"button\" class=\"btn btn-danger\"><i class=\"bi bi-trash text-light\"></i> Obriši</button></td>\nimport axios from \"axios\";\nexport default {\n  name: 'PregledKorpe',\n  data: function () {\n    return {\n      mojaKorpa: {\n        artikli: [{\n          naziv: \"\",\n          cena: 0,\n          kolicina: 0\n        }],\n        ukupnaCena: 0\n      }\n    };\n  },\n  mounted: function () {\n    // ovde kao callback funkciju odmah poziva i pravi employee\n    axios.get('http://localhost:8081/api/pregledKorpe/', {\n      withCredentials: true\n    }).then(res => {\n      console.log(res.data);\n      this.mojaKorpa = res.data;\n    }).catch(err => {\n      // todo neovlascen pristup\n      console.log(err);\n    });\n  },\n  methods: {\n    deleteArtikal: function (naziv) {\n      axios.delete('http://localhost:8081/api/izbaciIzKorpeNazivomArtikla/' + naziv, {\n        withCredentials: true\n      }).then(res => {\n        console.log(res.data);\n        window.location.reload();\n      }).catch(err => {\n        // todo neovlascen pristup\n        console.log(err);\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AAmEA;AAEA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;EACXC,IAAI,EAAE,cADK;EAEXC,IAAI,EAAE,YAAW;IACb,OAAO;MACHC,SAAS,EAAE;QACTC,OAAO,EAAE,CACL;UACIC,KAAK,EAAE,EADX;UAEIC,IAAI,EAAE,CAFV;UAGIC,QAAQ,EAAE;QAHd,CADK,CADA;QAQTC,UAAU,EAAE;MARH;IADR,CAAP;EAYH,CAfU;EAgBXC,OAAO,EAAE,YAAW;IAChB;IAEAT,KAAI,CACCU,GADL,CACS,yCADT,EACoD;MAACC,eAAe,EAAE;IAAlB,CADpD,EAEKC,IAFL,CAEWC,GAAD,IAAS;MACXC,OAAO,CAACC,GAAR,CAAYF,GAAG,CAACX,IAAhB;MACA,KAAKC,SAAL,GAAiBU,GAAG,CAACX,IAArB;IACJ,CALJ,EAMKc,KANL,CAMYC,GAAD,IAAQ;MAAE;MACbH,OAAO,CAACC,GAAR,CAAYE,GAAZ;IACH,CARL;EASH,CA5BU;EA6BXC,OAAO,EAAE;IACPC,aAAa,EAAE,UAASd,KAAT,EAAgB;MAC3BL,KAAI,CACDoB,MADH,CACU,2DAA2Df,KADrE,EAC4E;QAACM,eAAe,EAAE;MAAlB,CAD5E,EAEGC,IAFH,CAESC,GAAD,IAAS;QACXC,OAAO,CAACC,GAAR,CAAYF,GAAG,CAACX,IAAhB;QACAmB,MAAM,CAACC,QAAP,CAAgBC,MAAhB;MACJ,CALF,E