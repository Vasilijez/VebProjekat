{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: 'KupacView',\n  data: function () {\n    return {\n      porudzbine: [{\n        artikli: [],\n        restoran: {},\n        datum_vreme: \"\",\n        cena: 0,\n        status: \"\"\n      }]\n    };\n  },\n  mounted: function () {\n    // ovde kao callback funkciju odmah poziva i pravi employee\n    axios.get('http://localhost:8081/api/porudzbine/', {\n      withCredentials: true\n    }).then(res => {\n      console.log(res.data);\n      this.porudzbine = res.data;\n    }).catch(err => {\n      // todo neovlascen pristup\n      console.log(err);\n    });\n  },\n  methods: {\n    ostaviKomentar: function (id) {\n      //this.restoran_naziv.naziv = naziv;\n      console.log(id);\n      axios.get('http://localhost:8081/api/dodajKomentar/' + id, {\n        withCredentials: true\n      }).then(res => {\n        console.log(res);\n        window.location.reload();\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AA2GA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;EACXC,IAAI,EAAE,WADK;EAEXC,IAAI,EAAE,YAAW;IACb,OAAO;MACHC,UAAU,EAAE,CAAC;QACDC,OAAO,EAAE,EADR;QAEDC,QAAQ,EAAE,EAFT;QAGDC,WAAW,EAAE,EAHZ;QAIDC,IAAI,EAAE,CAJL;QAKDC,MAAM,EAAE;MALP,CAAD;IADT,CAAP;EASH,CAZU;EAaXC,OAAO,EAAE,YAAW;IAChB;IAEAT,KAAI,CACCU,GADL,CACS,uCADT,EACkD;MAACC,eAAe,EAAE;IAAlB,CADlD,EAEKC,IAFL,CAEWC,GAAD,IAAS;MACXC,OAAO,CAACC,GAAR,CAAYF,GAAG,CAACX,IAAhB;MACA,KAAKC,UAAL,GAAkBU,GAAG,CAACX,IAAtB;IACJ,CALJ,EAMKc,KANL,CAMYC,GAAD,IAAQ;MAAE;MACbH,OAAO,CAACC,GAAR,CAAYE,GAAZ;IACH,CARL;EASH,CAzBU;EA0BbC,OAAO,EAAE;IACHC,cAAc,EAAE,UAAUC,EAAV,EAAc;MAC5B;MACAN,OAAO,CAACC,GAAR,CAAYK,EAAZ;MACFpB,KAAI,CACCU,GADL,CACS,6CAA6CU,EADtD,EAC0D;QAACT,eAAe,EAAE;MAAlB,CAD1D,EAEKC,IAFL,CAEUC,GAAE,IAAK;QACTC,OAAO,CAACC,GAAR,CAAYF,GAAZ;QACAQ,MAAM,CAACC,QAAP,CAAgBC,MAAhB;MACH,CALL,EAMKP,KANL,CAMWQ,KAAI,IAAK;QACZV,OAAO,CAACC,GAAR,CAAYS,KAAZ;MACH,CARL;IASA;EAbG;AA1BI,CAAf","names":["axios","name","data