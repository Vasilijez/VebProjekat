{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: \"LoginView\",\n  data: function () {\n    return {\n      uloga: \"neovlascen_pristup\",\n      user: {\n        korisnickoIme: \"\",\n        lozinka: \"\"\n      },\n      errMsg: false\n    };\n  },\n  mounted: function () {\n    axios // uloga\n    .get('http://localhost:8081/api/vratiUlogu/', {\n      withCredentials: true\n    }).then(res => {\n      this.uloga = res.data;\n    }).catch(err => {\n      // todo neovlascen pristup\n      console.log(err);\n    });\n  },\n  methods: {\n    handleSubmit: function () {\n      axios.post(\"http://localhost:8081/api/login\", this.user, {\n        withCredentials: 'include'\n      }).then(res => {\n        if (res.data.uloga === 'Admin') {\n          this.$router.push(\"/admin\"); // bolje mozda redirektovati na /profil\n        } else if (res.data.uloga === 'Menadzer') {\n          this.$router.push(\"/menadzer\");\n        } else if (res.data.uloga === 'Kupac') {\n          this.$router.push(\"/kupac\");\n        } else if (res.data.uloga === 'Dostavljac') {\n          this.$router.push(\"/dostavljac\");\n        }\n      }).catch(error => {\n        this.errMsg = true;\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AA2EA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;EACXC,IAAI,EAAE,WADK;EAEXC,IAAI,EAAE,YAAW;IACb,OAAO;MACHC,KAAK,EAAE,oBADJ;MAEHC,IAAI,EAAE;QACFC,aAAa,EAAE,EADb;QAEFC,OAAO,EAAE;MAFP,CAFH;MAMHC,MAAM,EAAE;IANL,CAAP;EAQH,CAXU;EAYXC,OAAO,EAAE,YAAW;IAChBR,KAAI,CAAE;IAAF,CACCS,GADL,CACS,uCADT,EACkD;MAACC,eAAe,EAAE;IAAlB,CADlD,EAEKC,IAFL,CAEWC,GAAD,IAAS;MACX,KAAKT,KAAL,GAAaS,GAAG,CAACV,IAAjB;IACJ,CAJJ,EAKKW,KALL,CAKYC,GAAD,IAAQ;MAAE;MACbC,OAAO,CAACC,GAAR,CAAYF,GAAZ;IACH,CAPL;EAQH,CArBU;EAsBXG,OAAO,EAAE;IACLC,YAAY,EAAE,YAAW;MACrBlB,KAAI,CACHmB,IADD,CACM,iCADN,EACyC,KAAKf,IAD9C,EACoD;QAACM,eAAe,EAAC;MAAjB,CADpD,EAECC,IAFD,CAEMC,GAAE,IAAK;QACT,IAAIA,GAAG,CAACV,IAAJ,CAASC,KAAT,KAAmB,OAAvB,EAAgC;UAC5B,KAAKiB,OAAL,CAAaC,IAAb,CAAkB,QAAlB,EAD4B