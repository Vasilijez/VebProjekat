{"ast":null,"code":"import axios from \"axios\";\nimport $ from 'jquery';\nexport default {\n  name: \"IzaberiMenadzera\",\n  data: function () {\n    return {\n      menadzeri: [],\n      restorani: [],\n      menadzer: {\n        naziv: \"\"\n      },\n      restoran: {\n        naziv: \"\"\n      }\n    };\n  },\n  mounted: function () {\n    axios.get(\"http://localhost:8081/api/menadzeri\", {\n      withCredentials: 'include'\n    }).then(res => {\n      console.log(res.data);\n      this.menadzeri = res.data;\n    }).catch(err => {\n      // todo neovlascen pristup\n      console.log(err);\n    });\n    fetch('http://localhost:8081/api/restorani').then(response => response.json()).then(data => {\n      console.log(\"Success:\", data);\n      this.restorani = data;\n    }) // bitno samo this.emplyees = data\n    .catch(error => {\n      console.error(\"Error:\", error);\n    });\n  },\n  methods: {\n    povezi: function () {\n      console.log(this.menadzer.naziv, this.restoran.naziv);\n    }\n  }\n};","map":{"version":3,"mappings":"AAwHA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,CAAP,MAAc,QAAd;AACA,eAAe;EACXC,IAAI,EAAE,kBADK;EAEXC,IAAI,EAAE,YAAY;IACd,OAAO;MACHC,SAAS,EAAE,EADR;MAEHC,SAAS,EAAE,EAFR;MAGHC,QAAQ,EAAE;QAAEC,KAAK,EAAE;MAAT,CAHP;MAIHC,QAAQ,EAAE;QAAED,KAAK,EAAE;MAAT;IAJP,CAAP;EAMH,CATU;EAUXE,OAAO,EAAE,YAAY;IAErBT,KAAI,CACDU,GADH,CACO,qCADP,EAC8C;MAACC,eAAe,EAAC;IAAjB,CAD9C,EAEGC,IAFH,CAESC,GAAD,IAAS;MACbC,OAAO,CAACC,GAAR,CAAYF,GAAG,CAACV,IAAhB;MACA,KAAKC,SAAL,GAAiBS,GAAG,CAACV,IAArB;IACD,CALH,EAMGa,KANH,CAMUC,GAAD,IAAQ;MAAE;MACfH,OAAO,CAACC,GAAR,CAAYE,GAAZ;IACD,CARH;IAWAC,KAAK,CAAC,qCAAD,CAAL,CACKN,IADL,CACUO,QAAO,IAAKA,QAAQ,CAACC,IAAT,EADtB,EAEKR,IAFL,CAEUT,IAAG,IAAK;MAACW,OAAO,CAACC,GAAR,CAAY,UAAZ,EAAwBZ,IAAxB;MAA+B,KAAKE,SAAL,GAAiBF,IAAjB;IAAsB,CAFxE,EAE0E;IAF1E,CAGKa,KAHL,CAGYK,KAAD,IAAW;MAChBP,OAAO,CAACO,KAAR,CAAc,QAAd,EAAwBA,KAAxB;IACD,CALL;EAQC,CA/BU;EAgCXC,OAAO,EAAE;IACLC,MAAM,EAAE,YAAW;MACjBT,OAAO,CAACC,GAAR,CAAY,KAAKT,QAAL,CAAcC,KAA1B,EAAiC,KAAKC,QAAL,CAAcD,KAA/C;IACF;EAHK;AAhCE,CAAf","names":["axios","$","name","data","menadzeri","restorani","menadzer","naziv","restoran","mounted","get","withCredentials","then","res","console","log","catch","err","fetch","response","json","error","methods","povezi"],"sourceRoot":"","sources":["C:\\Users\\Vasilije\\Desktop\\Fakultet\\Druga godina\\4. semestar\\Veb programiranje\\front_projekat\\demo-front\\src\\views\\IzaberiMenadzeraView.vue"],"sourcesContent":["<template>\r\n\r\n\r\n\r\n\r\n<nav class=\"navbar navbar-expand-lg navbar-light bg-light\">\r\n  <div class=\"container-fluid\">\r\n    <a class=\"navbar-brand\" href=\"#\">Navbar</a>\r\n    <button class=\"navbar-toggler\" type=\"button\" data-bs-toggle=\"collapse\" data-bs-target=\"#navbarNav\" aria-controls=\"navbarNav\" aria-expanded=\"false\" aria-label=\"Toggle navigation\">\r\n      <span class=\"navbar-toggler-icon\"></span>\r\n    </button>\r\n    <div class=\"collapse navbar-collapse\" id=\"navbarNav\">\r\n      <ul class=\"navbar-nav\">\r\n        <li class=\"nav-item\">\r\n          <a class=\"nav-link active\" aria-current=\"page\" href=\"#\">Home</a>\r\n        </li>\r\n        <li class=\"nav-item\">\r\n          <a class=\"nav-link\" href=\"#\">Početna</a>\r\n        </li>\r\n        <li class=\"nav-item\">\r\n          <a class=\"nav-link\" href=\"#\">Odjavi se</a>\r\n        </li>\r\n        <li class=\"nav-item\">\r\n          <a class=\"nav-link disabled\" href=\"#\" tabindex=\"-1\" aria-disabled=\"true\">Disabled</a>\r\n        </li>\r\n      </ul>\r\n    </div>\r\n  </div>\r\n</nav>\r\n\r\n<h2>Menadzeri</h2>\r\n<table class=\"table table-hover table-bordered results\">\r\n  <thead>\r\n    <tr>\r\n      <th>#</th>\r\n      <th class=\"col-md-2 col-xs-5\">Korisničko ime</th>\r\n      <th class=\"col-md-2 col-xs-4\">Ime</th>\r\n      <th class=\"col-md-2 col-xs-3\">Prezime</th>\r\n      <th class=\"col-md-2 col-xs-3\">Pol</th>\r\n      <th class=\"col-md-2 col-xs-3\">Uloga</th>\r\n      <th class=\"col-md-2 col-xs-3\">Datum rođenja</th>\r\n    </tr>\r\n    <tr class=\"warning no-result\">\r\n      <td colspan=\"4\"><i class=\"fa fa-warning\"></i> No result</td>\r\n    </tr>\r\n  </thead>\r\n  <tbody>\r\n    <tr  v-for=\"(korisnik,i) in menadzeri\" :key=\"i\">\r\n            \r\n            <th v-if=\"korisnik.uloga == 'Menadzer'\" scope=\"row\">{{i}}</th>\r\n            <td v-if=\"korisnik.uloga == 'Menadzer'\">{{korisnik.korisnickoIme}}</td>\r\n            <td v-if=\"korisnik.uloga == 'Menadzer'\">{{korisnik.ime}}</td>\r\n            <td v-if=\"korisnik.uloga == 'Menadzer'\">{{korisnik.prezime}}</td>\r\n            <td v-if=\"korisnik.uloga == 'Menadzer'\">{{korisnik.p}}</td>\r\n            <td v-if=\"korisnik.uloga == 'Menadzer'\">{{korisnik.uloga}}</td>\r\n            <td v-if=\"korisnik.uloga == 'Menadzer' \">{{korisnik.datum_rodjenja.substring(0,10)}}</td>\r\n    </tr>\r\n  </tbody>\r\n</table>\r\n\r\n<h2>Restorani</h2>\r\n<table class=\"table table-hover table-bordered results\">\r\n  <thead>\r\n    <tr>\r\n      <th>#</th>\r\n      <th class=\"col-md-2 col-xs-3\">Naziv restorana</th>\r\n      <th class=\"col-md-2 col-xs-3\">Tip restorana</th>\r\n      <th class=\"col-md-2 col-xs-3\">Adresa</th>\r\n      <th class=\"col-md-2 col-xs-3\">Geografska širina</th>\r\n      <th class=\"col-md-2 col-xs-3\">Geografska dužina</th>\r\n    </tr>\r\n    <tr class=\"warning no-result\">\r\n      <td colspan=\"4\"><i class=\"fa fa-warning\"></i> No result</td>\r\n    </tr>\r\n  </thead>\r\n  <tbody>\r\n    <tr  v-for=\"(restoran,i) in restorani\" :key=\"i\">\r\n            <th scope=\"row\">{{i}}</th>\r\n            <td>{{restoran.naziv}}</td>\r\n            <td>{{restoran.tipRestorana}}</td>\r\n            <td>{{restoran.lokacija.adresa}}</td>\r\n            <td>{{restoran.lokacija.sirina}}</td>\r\n            <td>{{restoran.lokacija.duzina}}</td>\r\n    </tr>\r\n  </tbody>\r\n</table>\r\n\r\n<label for=\"html\">Ponuđeni menadžeri</label>\r\n<select v-model=\"menadzer.naziv\" class=\"form-select form-select mb-3\" aria-label=\".form-select-lg example\">\r\n  <option selected>Izaberite željenog</option>\r\n  <option  v-for=\"(korisnik,i) in menadzeri\" :key=\"i\" :value=\"korisnik.korisnickoIme\">{{korisnik.korisnickoIme}}</option>\r\n</select>\r\n<label for=\"html\">Ponuđeni restorani</label>\r\n<select v-model=\"this.restoran.naziv\" class=\"form-select form-select mb-3\" aria-label=\".form-select-lg example\">\r\n  <option selected>Izaberite željeni</option>\r\n  <option v-for=\"(restoran,j) in restorani\" :key=\"j\" :value=\"restoran.naziv\">{{restoran.naziv}}</option>\r\n</select>\r\n<input @click = \"povezi()\" class=\"btn btn-success\" type=\"submit\" value=\"Potvrdi dodelu\">\r\n\r\n\r\n    <br>\r\n    <br>\r\n    <button type=\"button\" class=\"btn btn-primary btn-block me-4\" @click=\"this.$router.push('/kreiraj-menadzera')\">Kreiraj menadzera</button>\r\n    <br>\r\n    <button type=\"button\" class=\"btn btn-primary btn-block me-4\" @click=\"this.$router.push('/kreiraj-dostavljaca')\">Kreiraj dostavljaca</button>\r\n    <br>\r\n    <button type=\"button\" class=\"btn btn-primary btn-block me-4\" @click=\"this.$router.push('/kreiraj-restoran')\">Kreiraj restoran</button>\r\n    <br>\r\n    <button type=\"button\" class=\"btn btn-primary btn-block me-4\" @click=\"this.$router.push('/obrisi-restoran')\">Obrisi restoran</button>\r\n    <br>\r\n    <button type=\"button\" class=\"btn btn-primary btn-block me-4\" @click=\"this.$router.push('/izaberi-menadzera')\">Izaberi menadžera</button>\r\n\r\n\r\n\r\n\r\n</template>\r\n\r\n\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport $ from 'jquery';\r\nexport default {\r\n    name: \"IzaberiMenadzera\",\r\n    data: function () {\r\n        return {\r\n            menadzeri: [],\r\n            restorani: [],\r\n            menadzer: { naziv: \"\" },\r\n            restoran: { naziv: \"\" }\r\n        };\r\n    },\r\n    mounted: function () {\r\n      \r\n    axios\r\n      .get(\"http://localhost:8081/api/menadzeri\", {withCredentials:'include'})\r\n      .then((res) => {\r\n        console.log(res.data)\r\n        this.menadzeri = res.data\r\n      })\r\n      .catch((err) =>{ // todo neovlascen pristup\r\n        console.log(err)\r\n      })\r\n\r\n\r\n    fetch('http://localhost:8081/api/restorani')\r\n        .then(response => response.json())\r\n        .then(data => {console.log(\"Success:\", data); this.restorani = data}) // bitno samo this.emplyees = data\r\n        .catch((error) => {\r\n          console.error(\"Error:\", error);\r\n        })\r\n        ;\r\n    \r\n    },\r\n    methods: {\r\n        povezi: function() {\r\n          console.log(this.menadzer.naziv, this.restoran.naziv)\r\n        }\r\n    }\r\n\r\n}\r\n   \r\n</script>\r\n\r\n<style>\r\n\r\nbody{\r\n  padding:20px 20px;\r\n}\r\n\r\n.results tr[visible='false'],\r\n.no-result{\r\n  display:none;\r\n}\r\n\r\n.results tr[visible='true']{\r\n  display:table-row;\r\n}\r\n\r\n.counter{\r\n  padding:8px; \r\n  color:#ccc;\r\n}\r\n\r\n.btn btn-primary {\r\n margin-right: 4px;\r\n}\r\n\r\n/* Pen-specific styles */\r\nhtml, body { \r\n  color: #fff;\r\n  height: 100%;\r\n}\r\n\r\n/* Pattern styles */\r\n.container {\r\n  background: linear-gradient(\r\n    to right, \r\n    #ff9e2c 0%, \r\n    #ff9e2c 50%, \r\n    #b6701e 50%, \r\n    #b6701e 100%\r\n  );\r\n  height: 100%;\r\n  width: 100%;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}