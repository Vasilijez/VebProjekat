 return {\r\n            korisnici: [],\r\n            restorani: []\r\n        };\r\n    },\r\n    mounted: function () {\r\n    axios\r\n      .get(\"http://localhost:8081/api/admin\", {withCredentials:'include'})\r\n      .then((res) => {\r\n        this.korisnici = res.data\r\n      })\r\n      .catch((err) =>{ // todo neovlascen pristup\r\n        console.log(err)\r\n      })\r\n\r\n    fetch('http://localhost:8081/api/restorani')\r\n        .then(response => response.json())\r\n        .then(data => {console.log(\"Success:\", data); this.restorani = data}) // bitno samo this.emplyees = data\r\n        .catch((error) => {\r\n          console.error(\"Error:\", error);\r\n        })\r\n        ;\r\n    \r\n    },\r\n    methods: function() {\r\n        \r\n    }\r\n\r\n}\r\n   \r\n</script>\r\n\r\n<style>\r\n\r\nbody{\r\n  padding:20px 20px;\r\n}\r\n\r\n.results tr[visible='false'],\r\n.no-result{\r\n  display:none;\r\n}\r\n\r\n.results tr[visible='true']{\r\n  display:table-row;\r\n}\r\n\r\n.counter{\r\n  padding:8px; \r\n  color:#ccc;\r\n}\r\n\r\n.btn btn-primary {\r\n margin-right: 4px;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}