{"ast":null,"code":"import axios from \"axios\";\nimport $ from 'jquery';\nexport default {\n  name: \"IzaberiMenadzera\",\n  data: function () {\n    return {\n      korisnici: [],\n      restorani: []\n    };\n  },\n  mounted: function () {\n    axios.get(\"http://localhost:8081/api/admin\", {\n      withCredentials: 'include'\n    }).then(res => {\n      this.korisnici = res.data;\n    }).catch(err => {\n      // todo neovlascen pristup\n      console.log(err);\n    });\n    fetch('http://localhost:8081/api/restorani').then(response => response.json()).then(data => {\n      console.log(\"Success:\", data);\n      this.restorani = data;\n    }) // bitno samo this.emplyees = data\n    .catch(error => {\n      console.error(\"Error:\", error);\n    });\n  },\n  methods: function () {}\n};","map":{"version":3,"mappings":"AA6EA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,CAAP,MAAc,QAAd;AACA,eAAe;EACXC,IAAI,EAAE,kBADK;EAEXC,IAAI,EAAE,YAAY;IACd,OAAO;MACHC,SAAS,EAAE,EADR;MAEHC,SAAS,EAAE;IAFR,CAAP;EAIH,CAPU;EAQXC,OAAO,EAAE,YAAY;IACrBN,KAAI,CACDO,GADH,CACO,iCADP,EAC0C;MAACC,eAAe,EAAC;IAAjB,CAD1C,EAEGC,IAFH,CAESC,GAAD,IAAS;MACb,KAAKN,SAAL,GAAiBM,GAAG,CAACP,IAArB;IACD,CAJH,EAKGQ,KALH,CAKUC,GAAD,IAAQ;MAAE;MACfC,OAAO,CAACC,GAAR,CAAYF,GAAZ;IACD,CAPH;IASAG,KAAK,CAAC,qCAAD,CAAL,CACKN,IADL,CACUO,QAAO,IAAKA,QAAQ,CAACC,IAAT,EADtB,EAEKR,IAFL,CAEUN,IAAG,IAAK;MAACU,OAAO,CAACC,GAAR,CAAY,UAAZ,EAAwBX,IAAxB;MAA+B,KAAKE,SAAL,GAAiBF,IAAjB;IAAsB,CAFxE,EAE0E;IAF1E,CAGKQ,KAHL,CAGYO,KAAD,IAAW;MAChBL,OAAO,CAACK,KAAR,CAAc,QAAd,EAAwBA,KAAxB;IACD,CALL;EAQC,CA1BU;EA2BXC,OAAO,EAAE,YAAW,CAEpB;AA7BW,CAAf","names":["axios","$","name","data","korisnici","restorani","mounted","get","withCredentials","then","res","catch","err","console","log","fetch","response","json","error","methods"],"sourceRoot":"","sources":["C:\\Users\\Vasilije\\Desktop\\Fakultet\\Druga godina\\4. semestar\\Veb programiranje\\front_projekat\\demo-front\\src\\views\\HomeView.vue"],"sourcesContent":["<template>\n\n<nav class=\"navbar navbar-expan