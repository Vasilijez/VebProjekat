{"ast":null,"code":"import axios from \"axios\";\nimport $ from 'jquery';\nexport default {\n  name: \"HomeView\",\n  data: function () {\n    return {\n      restorani: []\n    };\n  },\n  mounted: function () {\n    $(document).ready(function () {\n      $(\".search\").keyup(function () {\n        var searchTerm = $(\".search\").val();\n        var listItem = $('.results tbody').children('tr');\n        var searchSplit = searchTerm.replace(/ /g, \"'):containsi('\");\n        $.extend($.expr[':'], {\n          'containsi': function (elem, i, match, array) {\n            return (elem.textContent || elem.innerText || '').toLowerCase().indexOf((match[3] || \"\").toLowerCase()) >= 0;\n          }\n        });\n        $(\".results tbody tr\").not(\":containsi('\" + searchSplit + \"')\").each(function (e) {\n          $(this).attr('visible', 'false');\n        });\n        $(\".results tbody tr:containsi('\" + searchSplit + \"')\").each(function (e) {\n          $(this).attr('visible', 'true');\n        });\n        var jobCount = $('.results tbody tr[visible=\"true\"]').length;\n        $('.counter').text(jobCount + ' item');\n\n        if (jobCount == '0') {\n          $('.no-result').show();\n        } else {\n          $('.no-result').hide();\n        }\n      });\n    });\n    axios.get(\"http://localhost:8081/api/restorani\").then(res => {\n      console.log(res.data);\n      this.restorani = res.data;\n    }).catch(err => {\n      // todo neovlascen pristup\n      console.log(err);\n    });\n  },\n  methods: function () {}\n};","map":{"version":3,"mappings":"AAsFA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,CAAP,MAAc,QAAd;AACA,eAAe;EACXC,IAAI,EAAE,UADK;EAEXC,IAAI,EAAE,YAAY;IACd,OAAO;MACHC,SAAS,EAAE;IADR,CAAP;EAGH,CANU;EAOXC,OAAO,EAAE,YAAY;IAErBJ,CAAC,CAACK,QAAD,CAAD,CAAYC,KAAZ,CAAkB,YAAW;MAC/BN,CAAC,CAAC,SAAD,CAAD,CAAaO,KAAb,CAAmB,YAAY;QAC7B,IAAIC,UAAS,GAAIR,CAAC,CAAC,SAAD,CAAD,CAAaS,GAAb,EAAjB;QACA,IAAIC,QAAO,GAAIV,CAAC,CAAC,gBAAD,CAAD,CAAoBW,QAApB,CAA6B,IAA7B,CAAf;QACA,I