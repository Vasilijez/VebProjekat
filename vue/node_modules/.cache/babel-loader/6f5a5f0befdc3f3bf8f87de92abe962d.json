{"ast":null,"code":"export default {\n  name: \"LoginView\",\n  data: function () {\n    return {\n      user: {\n        korisnickoIme: \"\",\n        lozinka: \"\"\n      },\n      role: \"\",\n      status: 0\n    };\n  },\n  methods: {\n    handleSubmit: function () {\n      fetch('http://localhost:8081/api/login', {\n        method: \"POST\",\n        headers: {\n          Accept: \"application/json\",\n          \"Content-type\": \"application/json\"\n        },\n        body: JSON.stringify(this.user)\n      }).then(response => {\n        this.status = response.status;\n      }).then(data => {\n        console.log(\"Success:\", this.status, data);\n\n        if (data.uloga === 'Admin') {\n          this.$router.push(\"/admin\"); // bolje mozda redirektovati na /profil\n        } else if (data.uloga === 'Menadzer') {\n          this.$router.push(\"/menadzer\");\n        } else if (data.uloga === 'Kupac') {\n          this.$router.push(\"/kupac\");\n        } else if (data.uloga === 'Dostavljac') {\n          this.$router.push(\"/dostavljac\");\n        }\n      }).catch(error => {\n        // todo resiti preko exception-a\n        console.error(\"Error:\", error);\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AAeA,eAAe;EACXA,IAAI,EAAE,WADK;EAEXC,IAAI,EAAE,YAAW;IACb,OAAO;MACHC,IAAI,EAAE;QACFC,aAAa,EAAE,EADb;QAEFC,OAAO,EAAE;MAFP,CADH;MAKHC,IAAI,EAAE,EALH;MAMHC,MAAM,EAAE;IANL,CAAP;EAQH,CAXU;EAaXC,OAAO,EAAE;IACLC,YAAY,EAAE,YAAW;MACrBC,KAAK,CAAC,iCAAD,EAAoC;QACrCC,MAAM,EAAE,MAD6B;QAErCC,OAAO,EAAE;UACTC,MAAM,EAAE,kBADC;UAET,gBAAgB;QAFP,CAF4B;QAMrCC,IAAI,EAAEC,IAAI,CAACC,SAAL,CAAe,KAAKb,IAApB;MAN+B,CAApC,CAAL,CASKc,IATL,CASUC,QAAO,IAAM;QAAC,KAAKX,MAAL,GAAcW,QAAQ,CAACX,MAAvB;MAA+B,CATvD,EAUKU,IAVL,CAUUf,IAAG,IAAK;QACViB,OAAO,CAACC,GAAR,CAAY,UAAZ,EAAwB,KAAKb,MAA7B,EAAqCL,IAArC;;QACA,IAAIA,IAAI,CAACmB,KAAL,KAAe,OAAnB,EAA4B;UACxB,KAAKC,OAAL,CAAaC,IAAb,CAAkB,QAAlB,EADwB,CACI;QAChC,CAFA,MAEO,IAAIrB,IAAI,CAACmB,KAAL,KAAc,UAAlB,EAA8B;UACjC,KAAKC,OAAL,CAAaC,IAAb,