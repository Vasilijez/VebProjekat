{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: 'RestoranView',\n  data: function () {\n    return {\n      menadzer: {\n        restoran: {\n          lokacija: {\n            adresa: \"\",\n            sirina: \"\",\n            duzina: \"\"\n          }\n        },\n        porudzbinaList: []\n      } //restoran: { lokacija: { adresa: \"\", sirina: \"\", duzina: \"\"}}, // pravi gresku morao sam dodati\n\n    };\n  },\n  mounted: function () {\n    // ovde kao callback funkciju odmah poziva i pravi employee\n    axios.get('http://localhost:8081/api/menadzer/', {\n      withCredentials: true\n    }).then(res => {\n      console.log(res.data);\n      this.menadzer = res.data;\n    }).catch(err => {\n      // todo neovlascen pristup\n      console.log(err);\n    });\n  }\n};","map":{"version":3,"mappings":"AAuIA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;EACXC,IAAI,EAAE,cADK;EAEXC,IAAI,EAAE,YAAW;IACb,OAAO;MACHC,QAAQ,EAAE;QACNC,QAAQ,EAAE;UAAEC,QAAQ,EAAE;YAAEC,MAAM,EAAE,EAAV;YAAcC,MAAM,EAAE,EAAtB;YAA0BC,MAAM,EAAE;UAAlC;QAAZ,CADJ;QAENC,cAAc,EAAE;MAFV,CADP,CAID;;IAJC,CAAP;EAMH,CATU;EAUXC,OAAO,EAAE,YAAW;IAChB;IACAV,KAAI,CACCW,GADL,CACS,qCADT,EACgD;MAACC,eAAe,EAAE;IAAlB,CADhD,EAEKC,IAFL,CAEWC,GAAD,IAAS;MACXC,OAAO,CAACC,GAAR,CAAYF,GAAG,CAACZ,IAAhB;MACA,KAAKC,QAAL,GAAgBW,GAAG,CAACZ,IAApB;IACH,CALL,EAMKe,KANL,CAMYC,GAAD,IAAQ;MAAE;MACbH,OAAO,CAACC,GAAR,CAAYE,GAAZ;IACH,CARL;EAWJ;AAvBW,CAAf","names":["axios","name","data","menadzer","restoran","lokacija","adresa","sirina","duzina","porudzbinaList","mounted","get","withCredentials","then","res","console","log","catch","err"],"sourceRoot":"","sources":["C:\\Users\\Vasilije\\Desktop\\Fakultet\\Druga godina\\4. semestar\\Veb programiranje\\front_projekat\\demo-front\\src\\views\\MenadzerView.vue"],"sourcesContent":["<template>\r\n\r\n<link\r\n      rel=\"stylesheet\"\r\n      href=\"https://cdn.jsdelivr.net/npm/bootstrap-icons@1.3.0/font/bootstrap-icons.css\"\r\n    />\r\n\r\n<nav class=\"navbar navbar-expand-lg n